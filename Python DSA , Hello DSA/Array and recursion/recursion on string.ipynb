   "source": [
    "# remove a particular character\n",
    "def remove_ch(s,ch):\n",
    "    if len(s)==0:\n",
    "        return s\n",
    "    small_ans = remove_ch(s[1:],ch)\n",
    "    if s[0] == ch :\n",
    "        return small_ans\n",
    "    else:\n",
    "        return s[0]+small_ans\n"
   ]
  },
